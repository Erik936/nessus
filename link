#Checking all DLLs on a system to verify valid signature certificates

Get-Process | ForEach-Object {
    $_.Modules | ForEach-Object {
        $module = $_
        $path = $module.FileName
        if (Test-Path $path) {
            $sig = Get-AuthenticodeSignature $path
            [PSCustomObject]@{
                ProcessName = $_.ProcessName
                ModuleName  = $module.ModuleName
                Signed      = $sig.Status
                Path        = $path
            }
        }
    }
} | Format-Table -Property ProcessName, ModuleName, Signed, Path

#Seeing if there any unsigned DLLs on the system

Get-Process | ForEach-Object {
    $_.Modules | ForEach-Object {
        $module = $_
        $path = $module.FileName
        if (Test-Path $path) {
            $sig = Get-AuthenticodeSignature $path
            if ($sig.Status -ne 'Valid') {
                [PSCustomObject]@{
                    ProcessName = $_.ProcessName
                    ModuleName  = $module.ModuleName
                    Signed      = $sig.Status
                    Path        = $path
                }
            }
        }
    }
} | Format-Table -Property ProcessName, ModuleName, Signed, Path
