#Show what PIDs a DLL is associated with

# Specify the DLL path (update with your desired DLL)
$dllPath = "C:\Windows\System32\msvcrt.dll"

# Get the list of processes and their modules
Get-Process | ForEach-Object {
    $process = $_
    $process.Modules | ForEach-Object {
        $module = $_
        if ($module.FileName -eq $dllPath) {
            [PSCustomObject]@{
                ProcessName = $process.ProcessName
                PID         = $process.Id
                ModuleName  = $module.ModuleName
                Path        = $module.FileName
            }
        }
    }
} | Format-Table -Property ProcessName, PID, ModuleName, Path
